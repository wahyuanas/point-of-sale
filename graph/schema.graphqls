type User {
  id: Int!
  username: String!
  name: String!
  password: String!
  email: String!
  phonenumber: String!
}

type CommonOutput {
    status: Boolean!
    code: Int!
    message: String
}

input SignInInput {
    username: String!
    password: String!
}

type SignInOutput {
    respon: CommonOutput!
    user: User!

}

type Query {
  getAccount: User!
  getAccounts: [User!]!
}

type Mutation {
  SignIn(input: SignInInput!): SignInOutput!
}
